<%- include("partials/header.ejs"); -%> <% for(let panorama of panoramas) { %>
<div class="card" mb-3>
  <div class="row">
    <div class="col-md-4">
      <img class="img-fluid" alt="" src="<%= panorama.images[0].url %>" />
    </div>
    <div class="col-md-8">
      <div class="card-body">
        <h5 class="card-title"><%= panorama.title %></h5>
        <p class="card-text">
          <small class="text-muted"><%= panorama.location %> </small>
        </p>
        <a class="btn btn-primary" href="/<%= panorama.id %>"
          >View <%= panorama.title %>
        </a>
      </div>
    </div>
  </div>
</div>
<% } %> <%- include("partials/footer.ejs"); -%>
